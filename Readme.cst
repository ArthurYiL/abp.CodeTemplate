                #endregion
                
              <!--前端 js 开始-->
                 <% foreach (ColumnSchema column in t.Columns) { %>  
                     {
                        name: lng('<%=tableName%>_<%=column.Name%>'),
                        <%string fieldName=GetParamName(column.Name); %>
                        field: '<%=fieldName%>',
                        minWidth: 100
                    },
              <% }%>
                
             <!--前端 js  结束-->


                <% }%>
                
            
        }

}
    
